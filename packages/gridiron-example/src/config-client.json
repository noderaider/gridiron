{ "appName": "gridiron",
  "environment": "prod",
  "hostname": "noderaider.github.io",
  "port": 443,
  "path": "/gridiron",
  "state": {
    "meta": {
      "tokens": {
        "persist": {
          "type": "cookie",
          "name": "appidentity",
          "days": 14
        },
        "propTypes": {
          "access": "string",
          "refresh": "string"
        },
        "concatProps": ":",
        "jsonProps": false,
        "base64Props": true
      },
      "fingerprint": {
        "persist": {
          "type": "cookie",
          "name": "appfingerprint",
          "days": 365
        },
        "propTypes": "string",
        "jsonProps": false,
        "base64Props": true
      }
    }
  }
}
